<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Metadata> 
		[HostComponent("spark.components.Button")] 
	</fx:Metadata>
	
	<s:states>
		<s:State name="up"/>
		<s:State name="down" />
		<s:State name="over"/>
		<s:State name="disabled"/>
	</s:states>
	
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="0x1A1A1A" color.over="0x3A3A3A"/>
		</s:fill>
	</s:Rect>
	
	<s:Label id="labelDisplay" verticalCenter="0" horizontalCenter="0" paddingLeft="10" paddingRight="10" paddingTop="2"/>
		
	<s:layout>
		<s:BasicLayout/>
	</s:layout>

</s:Skin>
